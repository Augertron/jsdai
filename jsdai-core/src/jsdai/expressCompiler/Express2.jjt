TOKEN : /* spec case REFERENCE FROM - USE FROM */
{
	< REFERENCE2: "(*REFERENCE FROM*) USE" > 
	{ 
		if (Support.flag_no_special_reference_from) 
			matchedToken.kind = USE;
		else  
			matchedToken.kind = REFERENCE;
	}
