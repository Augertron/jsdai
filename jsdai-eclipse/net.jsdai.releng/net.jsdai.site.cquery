<cq:componentQuery xmlns:cq="http://www.eclipse.org/buckminster/CQuery-1.0" resourceMap="http://www.lksoft.lt/~jsdai/buckminster/lksoft-cvs-4.rmap">
    <cq:rootRequest name="net.jsdai.site" componentType="buckminster" versionDesignator="[1.0.0,1.0.1)" versionType="OSGi"/>
    <!--cq:property key="cvsMethod" value="ext"/>
    <cq:property key="cvsUser" value=""/-->
    <cq:property key="target.arch" value="*"/>
    <cq:property key="target.os" value="*"/>
    <cq:property key="target.ws" value="*"/>

    <!-- Specify feature versions -->
    <cq:advisorNode namePattern="^\Qnet.jsdai.express_compiler.feature\E$" componentType="eclipse.feature" useWorkspace="false" resolutionPrio="1,2,3" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.express_g.feature\E$" componentType="eclipse.feature" useWorkspace="false" resolutionPrio="1,2,3" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi"/>

    <!-- Specify plugin versions -->
    <cq:advisorNode namePattern="^\Qnet.jsdai.apache\E$" componentType="osgi.bundle" versionOverride="[2.1.1.qualifier,2.1.1.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.common\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.express_compiler_core\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.express_compiler\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.express_doc\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.express_g\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.ext_dict_lib\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.impexpWizards\E$" componentType="osgi.bundle" versionOverride="[4.1.3.qualifier,4.1.3.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern="^\Qnet.jsdai.runtime\E$" componentType="osgi.bundle" versionOverride="[4.1.201.qualifier,4.1.201.qualifier]" versionOverrideType="OSGi" useWorkspace="false" resolutionPrio="1,2,3"/>

    <cq:advisorNode namePattern="(net\.jsdai\..+|org\.eclipse\.update\.ui\.accessor)" useTargetPlatform="false" useWorkspace="false" resolutionPrio="1,2,3"/>
    <cq:advisorNode namePattern=".+" useWorkspace="false" resolutionPrio="1,2,3"/>
</cq:componentQuery>
