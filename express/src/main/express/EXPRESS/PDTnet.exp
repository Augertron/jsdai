 ENTITY collected_item_association
  SUBTYPE OF (item_definition_instance_relationship);
  SELF\item_definition_instance_relationship.relating : collection_definition;
 END_ENTITY;

 ENTITY collection_definition
  SUBTYPE OF (design_discipline_item_definition);
  purpose : OPTIONAL string_select;
 INVERSE
  collected_items : SET [2:?] OF collected_item_association FOR relating;
 END_ENTITY;


