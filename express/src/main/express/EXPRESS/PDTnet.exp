 TYPE activity_element_select = SELECT
      (property,specification_category,class_category_association,class_condition_association,
	   product_class,activity_method,item_instance,product_identification,
       document_representation,document_file,document,document_version,
       product_structure_relationship,item_definition_instance_relationship,
       complex_product,process_plan,property_value_association,
       process_operation_definition,design_discipline_item_definition,item,
       item_version,material,process_operation_occurrence,physical_instance,
       physical_assembly_relationship,specification_expression,specification,
	   class_inclusion_association,specification_inclusion,class_specification_association,
	   class_structure_relationship,configuration,design_constraint,item_instance_relationship,
	   manufacturing_configuration);
 END_TYPE;

 TYPE alias_select = SELECT
      (approval_status,classification_system,design_discipline_item_definition,
       document,document_representation,document_type_property,document_version
       ,general_classification,item,item_instance,item_version,organization,
       property,product_class,specification_category,physical_instance,
       complex_product,specification);
 END_TYPE;

 TYPE approval_element_select = SELECT
      (activity,activity_element,activity_method_assignment,
       classification_system,class_condition_association,
	   design_discipline_item_definition,document,
       document_file,document_version,document_representation,
       general_classification,item_definition_instance_relationship,
       item_instance,item_version,property,property_value_association,
       specification_category,class_category_association,product_class,material
       ,process_plan,project,physical_assembly_relationship,physical_instance,
       product_structure_relationship,complex_product,work_order,work_request,
	   specification_expression,specification,class_inclusion_association,
	   specification_inclusion,class_specification_association,class_structure_relationship,
	   configuration,design_constraint,item_instance_relationship,physical_instance_test_result,
	   manufacturing_configuration);
 END_TYPE;

 TYPE assigned_document_select = SELECT
      (document,document_file,document_representation,document_version);
 END_TYPE;

 TYPE change_relationship_select = SELECT
      (item_version_relationship,process_plan_relationship,
       process_operation_occurrence_relationship,complex_product_relationship,
	   design_constraint_relationship,replaced_usage_relationship);
 END_TYPE;

 TYPE class_source_select = SELECT
