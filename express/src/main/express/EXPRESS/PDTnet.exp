 ENTITY class_structure_relationship;
  related : product_function_component_select;
  relating : product_class;
  description : OPTIONAL string_select;
  relation_type : STRING;
 END_ENTITY;

 ENTITY classification_system;
  description : OPTIONAL string_select;
  id : STRING;
