  END_ENTITY;

  ENTITY concept_of_point_in_space
    SUBTYPE OF(class_of_point_in_space, concept_of_spatial_aspect);
  END_ENTITY;

  ENTITY concept_of_spatial_aspect
    SUPERTYPE OF (ONEOF(concept_of_point_in_space, concept_of_curve_in_space, concept_of_surface_in_space, concept_of_volume_in_space))
    SUBTYPE OF(concept_of_characteristic, class_of_spatial_aspect);
  END_ENTITY;

  ENTITY concept_of_curve_in_space
    SUBTYPE OF(class_of_curve_in_space, concept_of_spatial_aspect);
  END_ENTITY;

  ENTITY class_of_curve_in_space
    SUPERTYPE OF (ONEOF(concept_of_curve_in_space, common_curve_in_space))
    SUBTYPE OF(class_of_spatial_aspect);
  END_ENTITY;

  ENTITY common_curve_in_space
