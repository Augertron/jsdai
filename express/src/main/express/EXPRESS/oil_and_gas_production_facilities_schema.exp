  END_ENTITY;

  ENTITY common_compound_characteristic
    SUPERTYPE OF (ONEOF(common_compound_characteristic_point, common_compound_characteristic_curve, common_compound_characteristic_surface, common_compound_characteristic_region))
    SUBTYPE OF(common_characteristic, class_of_compound_characteristic);
  END_ENTITY;

  ENTITY class_of_compound_characteristic
