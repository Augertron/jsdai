SCHEMA oil_and_gas_production_facilities_schema;

(* ISO 15926-2 CD Version August 1999 *)

  TYPE activity_or_possessed_event_effect = SELECT
    (activity,
     possessed_event_effect);
  END_TYPE;

  TYPE class_or_possessed_characteristic = SELECT
    (class_of_characteristic,
     possessed_characteristic);
  END_TYPE;

  TYPE person_or_organization = SELECT
    (organization,
     person);
  END_TYPE;

  TYPE class_or_possessed_point_in_space = SELECT
    (class_of_point_in_space,
     possessed_point_in_space);
  END_TYPE;

  ENTITY activity
    SUBTYPE OF(individual);
  END_ENTITY;

  ENTITY individual
    ABSTRACT SUPERTYPE OF (ONEOF(activity, physical_object) ANDOR ONEOF(whole_individual, possessed_aspect) ANDOR ONEOF(specific_individual, typical_individual) ANDOR ONEOF(plural_individual, single_individual))
    SUBTYPE OF(application_object);
  END_ENTITY;

  ENTITY application_object
    ABSTRACT SUPERTYPE OF (ONEOF(class, individual, specialization_of_class, possession_of_aspect));
      exchange_identifier : OPTIONAL STRING;
  END_ENTITY;

  ENTITY class
    ABSTRACT SUPERTYPE OF (ONEOF(class_of_class, class_of_individual))
    SUBTYPE OF(application_object);
  END_ENTITY;

  ENTITY class_of_class
    SUBTYPE OF(class);
  END_ENTITY;

  ENTITY class_of_individual
    ABSTRACT SUPERTYPE OF (ONEOF(class_of_plural_individual, class_of_single_individual) ANDOR ONEOF(class_of_whole_individual, class_of_aspect) ANDOR ONEOF(class_of_activity, class_of_physical_object))
    SUBTYPE OF(class);
  END_ENTITY;

  ENTITY class_of_plural_individual
    SUBTYPE OF(class_of_individual);
  END_ENTITY;

  ENTITY class_of_single_individual
    SUBTYPE OF(class_of_individual);
  END_ENTITY;

  ENTITY class_of_whole_individual
    SUBTYPE OF(class_of_individual);
  END_ENTITY;

  ENTITY class_of_aspect
    ABSTRACT SUPERTYPE OF (ONEOF(class_of_characteristic, class_of_state, class_of_association, class_of_encoded_information, class_of_information, class_of_characteristic_value, class_of_symbol, class_of_identification_aspect) ANDOR ONEOF(concept_of_aspect, common_aspect))
    SUBTYPE OF(class_of_individual);
  END_ENTITY;

  ENTITY class_of_characteristic
    ABSTRACT SUPERTYPE OF (ONEOF(concept_of_characteristic, common_characteristic) ANDOR ONEOF(class_of_property, class_of_temporal_aspect, class_of_spatial_aspect, class_of_quality, class_of_compound_characteristic))
    SUBTYPE OF(class_of_aspect);
  END_ENTITY;

  ENTITY concept_of_characteristic
    SUPERTYPE OF (ONEOF(concept_of_property, concept_of_temporal_aspect, concept_of_quality, concept_of_compound_characteristic, concept_of_spatial_aspect))
    SUBTYPE OF(class_of_characteristic, concept_of_aspect);
  END_ENTITY;

  ENTITY concept_of_aspect
    SUPERTYPE OF (ONEOF(concept_of_association, concept_of_characteristic, concept_of_state, concept_of_symbol, concept_of_encoded_information, concept_of_information, concept_of_characteristic_value))
    SUBTYPE OF(class_of_aspect);
  END_ENTITY;

  ENTITY concept_of_association
    SUBTYPE OF(class_of_association, concept_of_aspect);
  END_ENTITY;

  ENTITY class_of_association
    SUPERTYPE OF (ONEOF(concept_of_association, common_association) ANDOR class_of_comparison_of_characteristic)
    SUBTYPE OF(class_of_aspect);
  END_ENTITY;

  ENTITY common_association
    ABSTRACT SUPERTYPE OF (ONEOF(common_composition_of_individual, common_purpose_of_possessed_aspect, common_derivation_of_characteristic, common_comparison_of_characteristic_with_reference, common_change_of_state, common_purpose_of_aspect, common_temporal_sequence_of_activity, common_installation_of_physical_object_for_functional_physical_object, common_possession_of_decomposition_aspect_of_physical_object, connection_of_physical_object_with_member_of_class_of_physical_object, usage_of_member_of_class_of_physical_object_in_connection, common_physical_feature_of_individual_physical_object, common_connection_of_physical_object, common_usage_of_physical_object_in_connection, common_segregation_of_physical_object, common_usage_of_intermediate_physical_object_for_segregation, common_possession_of_topologic_sequence_aspect_by_physical_object, common_topologic_sequence_of_physical_object, common_protection_of_physical_object, common_physical_object_following_route, common_source_of_route, common_destination_of_route, common_possession_of_routing_aspect_of_physical_object, common_information_about_member_of_class, common_reference_to_object_within_information, common_possession_of_aspect, common_conversion_of_encoded_information, common_component_of_location_of_point_in_space, common_involvement_of_physical_object_in_activity, involvement_of_physical_object_in_member_of_class_of_activity, installation_of_class_of_physical_object_for_functional_physical_object, common_role_in_life_of_physical_object, role_in_life_of_members_of_class_of_physical_object, common_information_about_application_object, common_inclusion_of_information, common_reference_to_member_of_class, common_reference_to_application_object, common_unique_context_for_reference, common_adjacency_of_point_in_space, composition_of_individual_from_members_of_class_of_individual, common_component_of_compound_characteristic, common_adjacency_of_compound_characteristic_point, common_functional_part_of_individual_functional_object, common_controlling_authority_of_reference, common_factorization_of_property_value, common_linear_conversion_of_property_value, common_displacement_of_point_in_space, common_reference_between_physical_object) ANDOR ONEOF(common_asserting_association, common_probalilistic_association, common_denying_association))
    SUBTYPE OF(class_of_association, common_aspect);
      role_1_life_lower_cardinality         : INTEGER;
      role_1_life_upper_cardinality         : OPTIONAL INTEGER;
      role_2_life_lower_cardinality         : INTEGER;
      role_2_life_upper_cardinality         : OPTIONAL INTEGER;
      role_2_simultaneous_upper_cardinality : OPTIONAL INTEGER;
      role_1_simultaneous_upper_cardinality : OPTIONAL INTEGER;
      role_2_simultaneous_lower_cardinality : INTEGER;
      role_1_simultaneous_lower_cardinality : INTEGER;
  END_ENTITY;

  ENTITY common_aspect
    SUPERTYPE OF (ONEOF(common_association, common_characteristic, common_state, common_decomposition_aspect_of_activity, common_decomposition_aspect_of_physical_object, common_topologic_sequence_aspect_of_physical_object, common_routing_aspect_of_physical_object, common_point_approximation_of_spatial_aspect, common_characteristic_value, common_point_approximation_of_compound_characteristic, common_symbol, common_information, common_encoded_information))
    SUBTYPE OF(class_of_aspect);
  END_ENTITY;

  ENTITY common_characteristic
    SUPERTYPE OF (ONEOF(common_property, common_temporal_aspect, common_quality, common_compound_characteristic, common_spatial_aspect))
    SUBTYPE OF(class_of_characteristic, common_aspect);
  END_ENTITY;

  ENTITY common_property
    SUBTYPE OF(common_characteristic, class_of_property);
  END_ENTITY;

  ENTITY class_of_property
    ABSTRACT SUPERTYPE OF (ONEOF(concept_of_property, common_property))
