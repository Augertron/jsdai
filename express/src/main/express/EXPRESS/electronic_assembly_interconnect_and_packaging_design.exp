       TYPEOF(SELF\measure_with_unit.unit_component);
  WR4: 'ELECTRONIC_ASSEMBLY_INTERCONNECT_AND_PACKAGING_DESIGN.NUMERIC_MEASURE' IN 
       TYPEOF(SELF\measure_with_unit.value_component);
