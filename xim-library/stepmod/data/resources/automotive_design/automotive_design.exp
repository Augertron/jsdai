     'LOCATION_SHAPE_REPRESENTATION' IN TYPEOF ( pdr.
     used_representation ) ) ) ) =1 ) AND ( SIZEOF ( QUERY ( pdr
     <* get_shape_aspect_property_definition_representations (
     SELF ) | ( 'AUTOMOTIVE_DESIGN.'+
     'LOCATION_SHAPE_REPRESENTATION' IN TYPEOF ( pdr.
     used_representation ) ) AND ( pdr. used_representation.name
     = 'floor location' ) ) ) =1 ) ;
 WR7: ( SELF.description<>'planar' ) OR ( SIZEOF ( QUERY ( pdr
     <* get_shape_aspect_property_definition_representations (
     SELF ) | ( 'AUTOMOTIVE_DESIGN.'+
     'DIRECTION_SHAPE_REPRESENTATION' IN TYPEOF ( pdr.
     used_representation ) ) ) ) =1 ) AND ( SIZEOF ( QUERY ( pdr
     <* get_shape_aspect_property_definition_representations (
     SELF ) | ( 'AUTOMOTIVE_DESIGN.'+
     'DIRECTION_SHAPE_REPRESENTATION' IN TYPEOF ( pdr.
     used_representation ) ) AND ( pdr. used_representation.name
     = 'floor normal' ) ) ) =1 ) ;
 WR8: ( SELF.description<>'through' ) OR ( SIZEOF ( QUERY ( pdr
     <* get_shape_aspect_property_definition_representations (
     SELF ) | ( 'AUTOMOTIVE_DESIGN.'+
     'SHAPE_REPRESENTATION_WITH_PARAMETERS' IN TYPEOF ( pdr.
     used_representation ) ) ) ) =0 ) ;
 WR9: ( SIZEOF ( QUERY ( sar <* USEDIN ( SELF ,
     'AUTOMOTIVE_DESIGN.SHAPE_ASPECT_RELATIONSHIP.RELATING_SHAPE_ASPECT'
     ) | ( 'AUTOMOTIVE_DESIGN.'+
     'FEATURE_COMPONENT_RELATIONSHIP' IN TYPEOF ( sar ) ) ) ) =1
     ) AND ( SIZEOF ( QUERY ( sar <* USEDIN ( SELF ,
     'AUTOMOTIVE_DESIGN.SHAPE_ASPECT_RELATIONSHIP.RELATING_SHAPE_ASPECT'
     ) | ( 'AUTOMOTIVE_DESIGN.'+
     'FEATURE_COMPONENT_RELATIONSHIP' IN TYPEOF ( sar ) ) AND (
