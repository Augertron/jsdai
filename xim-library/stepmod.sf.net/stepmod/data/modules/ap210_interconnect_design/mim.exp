SCHEMA Ap210_interconnect_design_mim;
	USE FROM Ap210_interconnect_design_for_microwave_mim;	-- ISO/TS 10303-1618

RULE spline_curve_population_constraint FOR
    (curve);
WHERE
  WR1:  SIZEOF( QUERY( cu <* curve |
('GEOMETRY_SCHEMA.B_SPLINE_CURVE' IN TYPEOF(cu))
              )) = 0;
END_RULE;

END_SCHEMA;

